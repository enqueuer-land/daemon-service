  "bin": {
    "enqueuer-daemon": "dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^13.1.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
